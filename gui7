local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("V1 | Made by jamaljackson6t9 | Just_Random#1188", "GrapeTheme")

-- Character Section
local Character = Window:NewTab("Character")
local CharacterSection = Character:NewSection("Character")

-- Tp Section
local Tp = Window:NewTab("Tp")
local TpSection = Tp:NewSection("Tp")

-- World Section
local World = Window:NewTab("World")
local WorldSection = World:NewSection("World")

-- Character Buttons
CharacterSection:NewButton("Ctrl + Click to Tp", "Ctrl + Left Click to Tp", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/JamalScripts/f-to-fly/main/ctrl%20%2B%20click%20to%20tp"))()
end)

CharacterSection:NewButton("Fly (F to toggle)", "you just fly...", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/JamalScripts/f-to-fly/main/f%20to%20fly"))()
end)

CharacterSection:NewButton("Human Flashlight", "you become light", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Human%20Flashlight.txt"))()
end)

CharacterSection:NewButton("Btools (client side only)", "edit the game", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/BTools.txt"))()
end)

CharacterSection:NewButton("Infinite Jump", "Can Jump in the air", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/JamalScripts/f-to-fly/main/infinite%20jump"))()
end)

CharacterSection:NewToggle("WalkSpeed", "go fast", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/JamalScripts/f-to-fly/main/walkspeed%20150"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/JamalScripts/f-to-fly/main/walkspeed%2016"))()
    end
end)

CharacterSection:NewToggle("JumpPower", "jump high", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 120
    else
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

CharacterSection:NewToggle("NoClip", "walk through walls", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/JamalScripts/f-to-fly/main/on%20noclip"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/JamalScripts/f-to-fly/main/off%20noclip"))()
    end
end)

