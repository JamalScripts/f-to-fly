-- World Buttons
WorldSection:NewToggle("Jesus (walk on water)", "Walk on water", function(state)
    if state then
        for _,v in pairs(game.Workspace.Water:GetChildren()) do
    v.CanCollide = true --Chnage this to false to be able to go in the water (so if u wanna have a toggle
    end
    game.StarterGui:SetCore("SendNotification", {Title="Solid Water Enabled"; Text="Fruity Pebbles#3302"; Duration=5;})
    else
        for _,v in pairs(game.Workspace.Water:GetChildren()) do
    v.CanCollide = false --Chnage this to false to be able to go in the water (so if u wanna have a toggle
    end
    game.StarterGui:SetCore("SendNotification", {Title="Solid Water Disabled"; Text="Fruity Pebbles#3302"; Duration=5;})
    end
end)

WorldSection:NewSlider("FullBright", "Makes u see in the dark", 10, 0, function(s)
    game.Lighting.Brightness = s
end)

WorldSection:NewToggle("No Shadows", "No Shadows", function(state)
    if state then
        game.Lighting.GlobalShadows = false
    else
        game.Lighting.GlobalShadows = true
    end
end)
